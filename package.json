  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "start:windows": "start-windows-enhanced.bat",
    "test:windows": "run-tests-windows.bat",
    "install:windows": "powershell -ExecutionPolicy Bypass -File windows-installer-enhanced.ps1",
    "build:windows": "npm run build && electron-builder --win --publish=never",
    "build:mac": "npm run build && node build-mac.js",
    "build:linux": "npm run build && node build-linux.js",
    "build:ios": "cd ios-app && chmod +x build-ios.sh && ./build-ios.sh",
    "build:android": "cd android-webview && chmod +x build-android.sh && ./build-android.sh",
    "build:android:windows": "cd android-webview && build-android.bat",
    "build:tauri": "node build-tauri.js",
    "build:tauri:dev": "node build-tauri.js dev",
    "build:extensions": "node build-extensions.js",
    "build:all": "node build-all-platforms.js",
    "dist:windows": "npm run build && electron-builder --win --publish=never",
    "dist:mac": "npm run build && electron-builder --mac --publish=never",
    "dist:linux": "npm run build && electron-builder --linux --publish=never",
    "dist:all": "npm run build && electron-builder --win --mac --linux --publish=never",
    "debug:windows": "debug-windows.bat",
    "deploy": "DEPLOY-NOW.bat",
    "cleanup": "CLEANUP-NOW.bat",
    "test:all": "node test-all-platforms.js",
    "test:platform": "node test-all-platforms.js",
    "release": "node release-manager.js release",
    "release:major": "node release-manager.js release --version-bump=major",
    "release:minor": "node release-manager.js release --version-bump=minor",
    "release:patch": "node release-manager.js release --version-bump=patch",
    "version:bump": "node release-manager.js bump",
    "version:show": "node release-manager.js version"
  },
